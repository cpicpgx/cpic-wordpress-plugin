<table class="table table-striped" id="tableGuidelines">
    <thead>
    <tr>
        <th style="width:60%;">Guidelines</th>
        <th style="width:20%;">Drugs</th>
        <th style="width:20%;">Genes</th>
    </tr>
    </thead>
    <tbody>
    {{#each this}}
    <tr>
        <td>
            <p><em><a href="{{guideline_url}}">{{guideline_name}}</a></em></p>
        </td>
        <td>{{#each drugs}}<div>{{this}}</div>{{/each}}</td>
        <td>{{#each genes}}<div><a href="{{url}}">{{symbol}}</a></div>{{/each}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>